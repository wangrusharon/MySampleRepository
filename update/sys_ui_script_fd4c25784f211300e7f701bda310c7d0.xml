        <description>Client-side logic for Angular App.</description>
        <global>false</global>
        <name>x_203882_angular_a.angular_app_client_lo</name>
        <script><![CDATA[var myAngularApp=angular.module('angularapp',[]);
myAngularApp.controller('AngularAppCtrl',function($scope){
	$scope.myVar='';
	$scope.i=0;
	$scope.currentURL=window.location.href;
	$scope.currentUser=g_user.userName;
	$scope.currentTime=new Date();
	$scope.clearTxt=function(){
		$scope.myVar='';
	$scope.i++;
		alert("the myVar value was cleared for "+$scope.i+" Times by "+g_user.userName);
	};
});]]></script>
